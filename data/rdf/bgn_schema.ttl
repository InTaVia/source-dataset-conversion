# Schema for BiographyNet as CIDOC/BIOC
#CLASSES


# 
bioc:dutch a bioc:Nationality;
	rdfs:label "Dutch".

# Specific event types (for states)

bgn:birth a crm:E55_Type;
	rdfs:label "Birth Event" .
bgn:death a crm:E55_Type;
	rdfs:label "Death Event" .
bgn:baptism	 a crm:E55_Type;
	rdfs:label "Baptism Event". 
bgn:funeral a crm:E55_Type;
	rdfs:label "Funeral Event" .
bgn:marriage a crm:E55_Type;
	rdfs:label "Marriage Event" .


bgn:occupationStateEvent  a crm:E55_Type;
	rdfs:label "Occupation Event" .
bgn:educationStateEvent  a crm:E55_Type;
	rdfs:label "Education Event" .
bgn:residenceStateEvent a crm:E55_Type;
	rdfs:label "Residence Event" .
bgn:floruitStateEvent a crm:E55_Type;
	rdfs:label "Floruit Event" .
bgn:faithStateEvent a crm:E55_Type;
	rdfs:label "Faith Event" .
bgn:claimtofameStateEvent a crm:E55_Type;
	rdfs:label "Claim to Fame Event" .
